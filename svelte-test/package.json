{
	"name": "svelte-test",
	"version": "0.0.1",
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@vitest/browser": "^2.0.0",
		"@vitest/ui": "^2.1.9",
		"playwright": "^1.40.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^7.0.4",
		"vitest": "^2.0.0"
	},
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"test:browser": "vitest --browser",
		"test:ui": "vitest --ui --browser"
	},
	"type": "module",
	"dependencies": {
		"sqlite-worker": "../pkg/sqlite-worker-0.1.0.tgz"
	}
}
